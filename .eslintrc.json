{
  "env": {
    "es6": true,
    "amd": true,
    "commonjs": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "error"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
